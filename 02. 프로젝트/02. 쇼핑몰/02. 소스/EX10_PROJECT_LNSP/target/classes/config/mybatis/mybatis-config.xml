<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "HTTP://mybatis.org/dtd/mybatis-3-config.dtd">

<configuration>
	<settings>
		<setting name="cacheEnabled" 			 value="false" />
		<setting name="useGeneratedKeys" 		 value="true" />
		<setting name="defaultExecutorType" 	 value="REUSE" />
		<setting name="mapUnderscoreToCamelCase" value="true"/>
	</settings>

	<typeAliases>
		<typeAlias type="com.lnsp.vo.CartVO" 		alias="cart" />
		<typeAlias type="com.lnsp.vo.ChangeVO" 		alias="change" />
		<typeAlias type="com.lnsp.vo.EstimateVO" 	alias="estimate" />
		<typeAlias type="com.lnsp.vo.FaqVO" 		alias="faq" />
		<typeAlias type="com.lnsp.vo.MemberVO" 		alias="member" />
		<typeAlias type="com.lnsp.vo.GoodsVO" 		alias="goods" />
		<typeAlias type="com.lnsp.vo.OrderInfoVO" 	alias="orderinfo" />
		<typeAlias type="com.lnsp.vo.TempVO" 		alias="temp" />
		<typeAlias type="com.lnsp.vo.QnaVO" 		alias="qna"/>
		<typeAlias type="com.lnsp.vo.AnswerVO" 		alias="answer"/>
	</typeAliases>

	<mappers>
		<mapper resource="config/mybatis/mapper/Faq_Sql.xml" />
		<mapper resource="config/mybatis/mapper/Goods_Sql.xml" />
		<mapper resource="config/mybatis/mapper/Member_Sql.xml" />
		<mapper resource="config/mybatis/mapper/OrderInfo_Sql.xml" />
		<mapper resource="config/mybatis/mapper/Temp_Sql.xml" />
		<mapper resource="config/mybatis/mapper/Manager_Sql.xml" />
		<mapper resource="config/mybatis/mapper/Cart_Sql.xml" />
		<mapper resource="config/mybatis/mapper/CScenter_Sql.xml" />
		<mapper resource="config/mybatis/mapper/Qna_Sql.xml" />
		<mapper resource="config/mybatis/mapper/Answer_Sql.xml" />
		<mapper resource="config/mybatis/mapper/Consult_Sql.xml" />
		<mapper resource="config/mybatis/mapper/Order_Sql.xml" />
		<mapper resource="config/mybatis/mapper/ManagerGoods_Sql.xml" />
	</mappers>
</configuration>